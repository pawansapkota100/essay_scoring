{
    "version": 2,
    "builds": [
      { "src": "mysite/wsgi.py", "use": "@vercel/python", "config": { "maxLambdaSize": "15mb", "runtime": "python3.9" } }
    ],
    "routes": [
      { "src": "/(.*)", "dest": "mysite/wsgi.py" }
    ],
    "functions": {
      "install": { "includeFiles": ["requirements.txt"], "runtime": "python3.9", "memory": 1024 },
      "build": { "use": "@vercel/python" },
      "serve": { "runtime": "python3.9", "memory": 1024 }
    }
  }
  